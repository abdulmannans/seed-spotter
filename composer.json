{
    "name": "abdulmannans/seed-spotter",
    "description": "A package to spot the discrepancies between database and seeders",
    "version": "1.0.0",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.0|^2.0",
        "pestphp/pest-plugin-laravel": "^1.0|^2.0",
        "orchestra/testbench-core": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abdulmannans\\SeedSpotter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abdulmannans\\SeedSpotter\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Abdul Mannan Siddiquei",
            "email": "samannnan1999@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Abdulmannans\\SeedSpotter\\SeedSpotterServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
